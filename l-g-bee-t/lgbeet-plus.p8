-- l g bee t +
-- made by pixienop~


-- design thoughts
-- instead of 'difficulty',
--  'speed' settings like:
--  - 'chill (1)'
--  - 'edm (3)'
--  - 'osu (6)'
--  etc :)

function _draw()
		cls(1)
		
		local st = t()
		
		print("l g bee t +", 5, 5, 13)
		
		-- draw bee
		local s = 1
		if (0 < sin(st*4.9)) then
				s = 3
		end
		
		pal(1,0)
		spr(s, 50+cos(st*.1)*30+cos(st*.34)*10, 50+sin(st*.3)*40.9, 2, 2)
		pal()
end
