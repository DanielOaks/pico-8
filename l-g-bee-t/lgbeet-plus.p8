-- tag
function update_tag()
		if 3.9 < t() then
				state = state_title
				_st_time = t()
		end
end

function draw_tag()
		cls(7)
		
		local st = current_st()
		
		pal(10,7)
		pal(9,7)
		pal(8,7)
		
		if 0.6 < st and st < 3.73 then
				pal(10,8)
		end
		if 1.65 < st and st < 3.75 then
				pal(9,2)
		end
		if 2.7 < st and st < 3.77 then
				pal(8,1)
		end
		
		spr(176, 26, 42, 9, 5)
		
		pal()
end

