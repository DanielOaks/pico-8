pico-8 cartridge // http://www.pico-8.com
version 16
__lua__
-- my little pony theme song
-- moosic tracked by me
-- sprites dun by me
-- "my little pony" font by marq from pouet~

t=0
start=0

function _init()
		music(0)
end

function _update()
		t += 0.02
		if (start < 140) then
				start += 2
		end
end

function _draw()
		cls()
		map()

		-- tag
		draw_tag()		
	
		x = sin(t)
		y = cos(t)
		jumpy_ponk = sin(t*2-0.7)

		-- heart
		draw_sprites(2, (127-16)+x*2, 1+y*2, 2, 2)

		spr(39, 20+x*1.1, 100+y*1.2) -- bubbles
		
		spr(32, 34+x*5, 104+jumpy_ponk*1.8) -- ponk
		spr(33, 46+x*1.5, 105, 1, 1, true) -- ponk

		spr(34, 56+x*2, 105) -- rares
		spr(35, 66+x*2.3, 105, 1, 1, true) -- twi
		
		spr(36, 79+x*2.2, 105) -- aj
		spr(37, 90+x*3, 105, 1, 1, true) -- dash
		
		-- cloud 1
		x = sin(t+0.2)
		draw_sprites(64, 10+x*2, 63, 4, 3)

		-- cloud 2
		x = sin(t+0.4)
		draw_sprites(70, 78+x*2, 70, 4, 3)

		if (start < 140) then
  		-- loading
  		if (start < 128) then
		  		rectfill(0, start, 127, 127, 1)
		  end
  		
  		-- loading trail
  		for i=0, 16 do
  				spr(112+i, i*8, start-8)
  		end
		end
end

function draw_tag()
		rectfill(0, 28, 127, 50, 0)
		rectfill(0, 29, 127, 49, 1)
		draw_sprites(144,8,32,14,2)
end

function draw_sprites(initial, x, y, w, h)
		for i=0, w-1 do
				for j=0, h-1 do
						spr(initial+i+16*j,
										x+i*8, y+j*8)
				end
		end
end

