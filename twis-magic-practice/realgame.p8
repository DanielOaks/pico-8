pico-8 cartridge // http://www.pico-8.com
version 18
__lua__
-- twi's magic practice
-- by pixienop

function _init()
		poke(0x5f2e,1) -- keep palette
end

function _update()
		st = scenetime()

		if state == gs_title then
			update_title()
		elseif state == gs_menu then
			update_menu()
		end
end

function _draw()
		if state == gs_title then
			draw_title()
		elseif state == gs_menu then
			draw_menu()
		end

		pal(4,132,1)
		pal(5,140,1)
		pal(6,141,1)
end

--

_scenetime_start = 0
function scenetime()
		return time() - _scenetime_start
end
-->8
-- game state

gs_title=1
gs_menu=2

state = gs_title
-->8
-- title

function draw_title()
	st=scenetime()
	cls(0)
	
	print("twi's magic practice",25,50,7)
	print("   pixienop 2020",25,59,5)
	
	spr(98+flr(st*4%2)*2,1,1,2,2)
end

function update_title()
	if 3<scenetime() then
		state=gs_menu
	end
end
