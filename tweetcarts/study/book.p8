__lua__
-- https://twitter.com/2darray/status/1109890993543884801

::_::
cls()

-- k = -1, then 1
for k=-1,1,2 do
	for j=8-8*k,8+8*k,k do
		x=59.5
		q=1-mid(2-t()/4%2-j/16,1)^2
		p=1-q
		y=84.5+(16-j)/2*p+q*j/2
		w=q/2
		for i=0,1,.02 do
			c=6+j%2
			if (j*i==0 or j>15) then
				c=4
			end
			if (sgn(x-60)==k) then
				line(x,y,x+10,y-40,c)
				line(x,y,x+10,y-41)
			end
			x+=cos(w)
			y+=sin(w)
			w-=p*.035*q*(1-i)
		end
	end
end

flip()goto _
