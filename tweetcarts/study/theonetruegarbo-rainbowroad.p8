pico-8 cartridge // http://www.pico-8.com
version 18
__lua__
a=0

function x(v,i)
	return 64+v*2*cos(a+(i/2)*0.1)
end

function y(v,i)
	return 64+v*sin(a+v*0.04)+(v/i*v)-30
end

::♥::
cls()

for v=1,50 do
	for i=1,20 do
		line(x(v,i),y(v,i),x(v+1,i+1),y(v+1,i+1),(i%7)+8)
	end
end
a+=0.01

flip()goto ♥
