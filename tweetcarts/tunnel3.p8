r=19.9
st=t()

for d=0,76 do
	xm=64-sin(st-d*.02)*5
	if d==0 then
		circfill(xm,64,r+5,8)
	end
	for i=0,1,0.05 do
		p=sin(i-d*.004)*sin(d*.02-i-st)
	
		nx=xm+sin(i)*(r+d-p*5*(1+d/50))+sin(d*.02+i+st)*4
		ny=64+cos(i)*(r+d+p*3*(1+d/50))+cos(d*.01+i+st)*4
		if i!=0 then
			line(x,y,nx,ny,p*8)
		end
		x,y=nx,ny
	end
end

