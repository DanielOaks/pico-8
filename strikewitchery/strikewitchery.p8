-- runtime
pnop_x = 28
pnop_y = 42
pnop_width = 9
pnop_height = 5
pnop_fadeout_started = false
pnop_fadeout_particles = {}
pnop_fadeout_stime = 0
title_wriggle_stime = 900
stpry_page_len = 3.75 -- secs

function _init()
		-- start music
		music(0)
end

function _update()
		demo_update()
end

function _draw()
		-- setup
		local dt = demotime()
		local st = scenetime()

		-- drawing
		sc = -1
		if current_scene == sc then
				-- do nothing
		end
		
		-- pixienop intro
		sc += 1
		if current_scene == sc then
				if 2.47 < st then
						-- fade out
						if not pnop_fadeout_started then
								pnop_fadeout_started = true
								pnop_fadeout_stime = st
								
								pnop_fadeout_particles = {}
								for x = 0, 127 do
										pnop_fadeout_particles[x] = -flr(rnd(100))
								end
						end

						color(0)
						for x = 0, 127 do
								line(x, -1, x, min(127, pnop_fadeout_particles[x] + (st-pnop_fadeout_stime)*170))
						end
						
				else
						pnop_fadeout_started = false
  				inpix = min(128, st * 150)
  
  				if 127 < inpix then
  						cls(0)
  						spr(176, pnop_x, pnop_y, pnop_width, pnop_height)
  				else
  						color(0)
  						pal(10, 7)
  						pal(9, 6)
  						pal(8, 5)
    				for i = 0, 64 do
     					-- left pixels
     					y = i+i
     					line(-1, y, inpix, y)
     					
     					a = {
     							7
     					}
     					
     					-- draw logo on top of left pixels
     					spry = flr(128-(pnop_height*8)+(y-pnop_y))
     					spry_on_spr = y-pnop_y
     					if 128-(pnop_height*8) <= spry and spry < 128 then
       					for x = -1, inpix do
       							sprx = flr(x + (128-inpix) - pnop_x)
       							if 0 < sprx and sprx <= pnop_width*8 then
       									c = sget(sprx, spry)
       									if c != 0 then
  		     									pset(x, y, c)
  		     							end
       							end
       					end
  		   					color(0)
  		   			end
     
     					-- right pixels
     					y += 1
     					line(128, y, 128-inpix, y)
     					   					
     					-- draw logo on top of right pixels
     					-- relying on previous calcs a lot
     					spry += 1
     					spry_on_spr += 1
     					if 128-(pnop_height*8) <= spry and spry < 128 then
       					for x = (128-inpix), 128 do
       							sprx = (x - pnop_x) - (128-inpix)
       							if 0 < sprx and sprx <= pnop_width*8 then
       									c = sget(sprx, spry)
       									pset(x, y, c)
       							end
       					end
  		   					color(0)
  		   			end
    				end
    		end
    end
				pal()
		end
		
		-- strikewitchery title page
		sc += 1
		if current_scene == sc then
				if scene_just_entered then
						continue_scene = 1
						title_wriggle_stime = 900
				elseif rs_just_entered and title_wriggle_stime == 900 then
						title_wriggle_stime = st
				end

				cls(0)
  		
  		-- wiggle
  		if title_wriggle_stime <= st then
  				local wt = st - title_wriggle_stime
  				local wt_lerp = wt * 0.29
  				if wt_lerp < 0.28 then
  						wt_lerp = 0
  				end
  				wt_lerp = 0
  				local wx = sin(dt*2.23)*4.9*wt_lerp
  				local wy = sin(dt*1.8)*4.9*wt_lerp
  				
  				--wx += max(0, wt-3)*max(0, wt-3)*350
  				camera(wx, wy)
  		end
				
				-- skirt fabric flapping
				border = 2				
				for y = 0, 12 do
						for x = 0, 12 do
								xmod = sin(y*0.17 + st*0.4)*7.8
								ymod = sin(x*0.07 + st*0.26)*4.8
						
								midx = (border + (x * 10) - 11*y) + xmod
								midy = (border + y * 10) + ymod
								
								radius = (cos(x*0.2+st*0.2) * sin(st*0.5))*3.7
								
								color(12)
								rectfill(midx-radius-1, midy-radius-1, midx+radius, midy+radius)
								color(1)
								rectfill(midx-radius, midy-radius, midx+radius, midy+radius)
						end
				end
				
				-- draw logo
				logox = 38
				logoy = 20
				
				color(0)
				clip(logox-10, logoy+1, 60, 25)
				circfill(logox+11, logoy+14, 14)
				clip()
				line(logox+2, logoy+2, logox+2, logoy+25)
				rectfill(logox+3, logoy+1, logox+45, logoy+26)
				circfill(logox+46, logoy+25, 8)
				circfill(logox+44, logoy+5, 14)
				
				spr(0, logox, logoy+2, 4, 3)
				spr(20, logox+32, logoy+11, 2, 2)
				spr(53, logox+40, logoy+27)
				spr(4, logox+48, logoy+19)
				spr(5, logox+48, logoy+27)
				spr(6, logox+33+sin(st*0.8)*.8, (logoy-7)+cos(st*0.8)*.8, 3, 3)

				color(6)
				print("credits 0", 47, 108)
				if title_wriggle_stime != 900 then
  				if flr((cos(st*1.049+0.463)+1)*0.9) % 2 == 0 then
  						color(1)
  						print("insert coin", 43, 81)
  						color(10)
  						print("insert coin", 43, 80)
  				end
  		end
		end
		
		-- intro scenes
		sc += 1
		if current_scene == sc then
				if scene_just_entered then
						continue_scene = 3
				end
				
				local pt = st -- picture time
				local sa_index = 1
				while stpry_page_len < pt do
						pt -= stpry_page_len
						sa_index += 1
				end
				local col = 0
		
				cls(0)
				color(0)
				for y = 1, 20 do
						for x = 1, 32 do
								drawx = (x-1)*4
								drawy = 16+((y-1)*4) + sin(st+0.05*x-0.04*y)*20.9*max(0,0.7-pt)
								i = (y-1)*32+x
								col = sub(sa[sa_index][1], i, i)
								fill = sub(sa[sa_index][2], i, i)
								if fill == "7" then
										fillp(0b0101101001011010)
								end
								rectfill(drawx, drawy, drawx+4, drawy+4, hc[col])
								fillp()
						end
				end
				
				if 0.8 < pt then
  				fillp(0b1100100100111001)
  				col = 12
  				if pt < 1 then
  						col = 1
  				end
  				rectfill(0,13,127,14,col)
  				rectfill(0,98,127,99,col)
  		end
  		
  		if 0.8 < pt then
  				col = 7
  				if pt < 1 then
  						col = 1
  				elseif pt < 1.2 then
  						col = 6
  				end
  				-- each line of text
  				local sa3 = sa[sa_index][3]
  				for i = 1, #sa3 do
		  				print(sa3[i][1], sa3[i][2], sa3[i][3], col)
  				end
  		end

				fillp()
		end
		
		-- normal wand rotating effect
		sc += 1
		if current_scene == sc then
				if scene_just_entered then
						continue_scene = 1
				end
				
				cls(1)
				print("effect here", 30+sin(st*0.5)*5.9, 80+cos(st*0.65)*6.9, 14)
		end
		
		-- set the final scene num dynamically
		-- so we don't need to stuff around so much
		if scene_just_entered then
				final_scene = sc
		end
		
		camera(0,0)  -- always reset cam

		-- print debug info
		if show_debug then
  		-- print scene time lol
  		color(1)
  		rectfill(103, 0, 128, 18)
  		rectfill(0, 0, 40, 18)
  		
  		color(14)
  		print("s:"..current_scene, 104, 1)
  		print(st, 104, 1+6)
  		print("c:"..continue_scene, 104, 1+12)

  		color(7)
  		print("cpu:" .. stat(1), 1, 1)
  		print("    " .. stat(2), 1, 1+6)
  		print("fps:" .. stat(7) .. "/" .. stat(8), 1, 1+6+6)
  end
		
		-- unset scene_just_entered
		if scene_just_entered then
				scene_just_entered = false
		end
		if rs_just_entered then
				rs_just_entered = false
		end
end
