version 18
__lua__
function _init()
	alt=false
	state="picker"
	p={}
	
	dragindex=-1
	dragoffset=0
	
	poke(0x5f2d, 1)
	setupsideprint()
end

function _update60()
	updatemouse()
	
	fillp(0b1100110000110011)
	rectfill(0,0,127,127,21)
	fillp()
	
	if btnp(5) then
		if state=="picker" then
			state="palette"
			exportpalette()
		else
			state="picker"
		end
	end
	
	if state=="picker" then
		updatepicker()
	elseif state=="palette" then
		updatepalette()
	end
	
	drawmouse()
end

function updatepicker()
	if btnp(4) then
		alt=not alt
	end
	
	if alt then
		for i=0,15 do
			pal(i,i+128,1)
		end
	else
		pal()
	end
	
	for i=0,15 do
		x=i%4*32+16
		y=flr(i/4)*32+16
		n=i
		if (alt) n+=128
		
		if click then
			dx=x-mousex
			dy=y-mousey
			if dx*dx+dy*dy<=10*10 then
				if inpal(n) then
					del(p,n)
				elseif #p<16 then
					add(p,n)
				end
				exportpalette()
			end
		end
		
		if inpal(n) then
			circfill(x,y,12,7)
			circfill(x,y,11,0)
		end
		circfill(x,y,10,i)
		nstr=n..""
		ocol=0
		if (i<3) ocol=6
		oprint(nstr,x+1-#nstr*2,y-2,i,ocol)
	end
end

function updatepalette()
	if #p==0 then
		return
	end
	
	if click then
		if mousex>0 and mousex<128 then
			if mousey>0 and mousey<128 then
				palx=flr(mousex/(128/#p))
				dragindex=palx+1
				dragoffset=palx*(128/#p)-mousex
			end
		end
	end
	
	dragmove=0
	if dragindex!=-1 then
		newindex=flr(mousex/(128/#p))+1
		newindex=mid(newindex,1,#p)
		dragmove=newindex-dragindex
		
		if clickhold==false then
			if dragmove!=0 then
				applydragmove()
			end
			dragindex=-1
		end
	end
	
	for j=0,1 do
		for i=1,#p do
			pal(i-1,p[i],1)
			xi=i
			if dragmove>0 then
				if dragindex<i and dragindex+dragmove>=i then
					xi-=1
				end
			elseif dragmove<0 then
				if dragindex>i and dragindex+dragmove<=i then
					xi+=1
				end
			end
			x1=(xi-1)/#p
			x2=xi/#p
			if dragindex==i then
				x1=(mousex+dragoffset)/127
				x2=x1+1/#p
			end
			
			if j==0 or dragindex==i then
				rectfill(1+x1*126,1,x2*126,126,i-1)
				
				if i<=#p/2 then
					ocol=#p-1
				else
					ocol=0
				end
				if #p>8 then
					sideprint(p[i],1+x1*126,125,i-1,ocol)
					sideprint(i-1,1+x1*126,10,i-1,ocol)
				else
					oprint(p[i],3+x1*126,120,i-1,ocol)
					oprint(i-1,3+x1*126,8,i-1,ocol)
				end
			end
		end
	end
end

function oprint(text,x,y,col,ocol)
	for u=-1,1 do
		for v=-1,1 do
			print(text,x+u,y+v,ocol)
		end
	end
	print(text,x,y,col)
end

function sideprint(n,x,y,col,ocol)
	pal(1,col)
	pal(2,ocol)
	nstr=n..""
	for i=1,#nstr do
		digit=n%10
		sspr(digit*7,0,7,5,x,y-4)
		n=flr(n/10)
		y-=4
	end
	pal(1,1)
	pal(2,2)
end

function setupsideprint()
	for i=0,9 do
		cls()
		oprint(i,1,1,1,2)
		for x=0,4 do
			for y=0,6 do
				sset(6-y+i*7,x,pget(x,y))
			end
		end
	end
end

function updatemouse()
	mousex=stat(32)
	mousey=stat(33)
	clickhold=(stat(34)%2==1)
	if clickhold==true and oldclick!=clickhold then
		click=true
	else
		click=false
	end
	oldclick=clickhold
end

function drawmouse()
	circ(mousex,mousey,1,0)
	pset(mousex,mousey,7)
end

function inpal(col)
	for i,c in pairs(p) do
		if c==col then
			return true
		end
	end
	return false
end

function applydragmove()
	local temp=p[dragindex]
	local dir=sgn(dragmove)
	for i=dragindex,dragindex+dragmove-dir,dir do
		p[i]=p[i+dir]
	end
	p[dragindex+dragmove]=temp
	
	exportpalette()
end

function exportpalette()
	local lua="_pal={"
	for i=1,#p do
		if i>1 then
			lua=lua..","
		end
		lua=lua..p[i]
	end
	lua=
		lua.."}"
		.."\n"
		.."for i,c in pairs(_pal) do"
		.."\n"
		.."\tpal(i-1,c,1)"
		.."\n"
		.."end"
		.."\n\n"
	
	printh(lua,"@clip")
end
