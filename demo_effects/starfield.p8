pico-8 cartridge // http://www.pico-8.com
version 18
__lua__
function _draw()
		cls()
		
		-- sky
		local skyy = 0
		fillp(0b0101101001011010)
		for x = 0, 127 do
				local y = skyy + sin(t()*0.2+x*0.004)*4.8 + cos(t()*0.023+x*0.03)*1.6
				y += abs((x/127)-0.5) * 16
				line(x, -1, x, y, 1)
		end
		fillp()
		
		-- stars
		local c = 1  -- colour
		local xrows = 18
		local yrows = 7
		for i = 1, yrows do -- back to front
				-- adjust width
				local width = i*0.6

				-- adjust colour
				c = 1
				if 6 < i then
						c = 7
				elseif 4 < i then
						c = 12
				end

				local y = 85 + i * 2.8

  		for j = 0, xrows do -- left to right
  				local distorty = (yrows - i) * (xrows/2 - j) * 1.8
  				local x = -80 + j * 16 + distorty + sin(t()*0.4 + i*0.08)*1.8
  				local thisy = y + cos(t()*0.7+j*0.1+i*0.14)*4.8
  				circ(x, thisy, width, c)
  		end
  end
end
